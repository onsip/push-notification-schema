{
  "schema": "http://json-schema.org/draft-07/schema#",
  "title": "OnSIP Push Notification for an INVITE",
  "definitions": {
    "payload": {
    "type": "object",
      "properties": {
        "server": { "type": "string" },
        "callid": {	"type": "string" },
        "fromtag": { "type": "string" }
      },
      "required": ["callid", "fromtag", "server"]
    }
  },
  "allOf": [
    { "$ref": "https://raw.githubusercontent.com/onsip/push-notification-schema/dev-kevin-test/schemas/base.json" },
    {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "payload": { "$ref": "#/definitions/payload" }
          },
          "required": ["payload"]
        }
      },
      "required": ["data"]
    }
  ]
}
