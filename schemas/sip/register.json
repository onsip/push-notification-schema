{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OnSIP Push Notification for an expiring registration",

  "definitions": {
    "payload": {
      "type": "object",
      "properties": {
        "server": { "type": "string" }
      },
      "required": ["server"]
    }
  },

  "allOf": [
    { "$ref": "https://raw.githubusercontent.com/onsip/push-notification-schema/dev-kevin-test/schemas/base.json" },
    {
      "type": "object",
      "properties": {
        "data": {
          "type": "object",
          "properties": {
            "payload": { "$ref": "#/definitions/payload" }
          },
          "required": ["payload"]
        }
      },
      "required": ["data"]
    }
  ]
}
